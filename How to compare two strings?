#include<stdio.h>
#include<string.h>

int main()
{
	#define SIZE1 10
	#define SIZE2 12

	char str1[SIZE1], str2[SIZE2];
	
	printf("Enter First String: \n");
	scanf("%s",str1);
	printf("Enter Second String: \n");
	scanf("%s",str2);

	if(strlen(str1) > strlen(str2))
	printf("string %s is longest than string %s.\n", str1,str2);
	else
	printf("string %s is longest than string. %s.\n", str2,str1);
	return 0;
}
	














